{
    "projectType": "library",
    "root": "libs/contrib/rxstate",
    "sourceRoot": "libs/contrib/rxstate/src",
    "prefix": "cloudextend",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:ng-packagr-lite",
            "outputs": ["dist/libs/contrib/rxstate"],
            "options": {
                "tsConfig": "libs/contrib/rxstate/tsconfig.lib.json",
                "project": "libs/contrib/rxstate/ng-package.json"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/contrib/rxstate/**/*.ts",
                    "libs/contrib/rxstate/**/*.html"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "libs/contrib/rxstate/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "generators": {},
    "tags": []
}
